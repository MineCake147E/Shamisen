<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
      <TargetFrameworks>net7.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Core interfaces of IO features for Shamisen.</Description>
    <PackageIcon>Shamisen-NuGetIcon.png</PackageIcon>
    <PackageTags>audio;csharp;cross platform;dotnet;dotnetcore;signal processing;audio player;dotnet core;audio library;dotnet standard;csharp library;codecs;audio io;io</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
    <Import Project="../Shamisen.Core.targets" />
    <ItemGroup>
        <None Include="README.md" Pack="true" PackagePath="" />
        <None Include="..\Shamisen-NuGetIcon.png" Pack="true" PackagePath="" />
    </ItemGroup>
    <ItemGroup>
      <ProjectReference Include="..\Shamisen.Core\Shamisen.Core.csproj" />
    </ItemGroup>
</Project>
