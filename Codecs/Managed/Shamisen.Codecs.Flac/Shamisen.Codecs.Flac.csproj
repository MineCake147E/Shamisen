<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net7.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageTags>audio;csharp;cross platform;dotnet;dotnetcore;signal processing;audio player;dotnet core;audio library;dotnet standard;csharp library;codecs;flac</PackageTags>
    <PackageIcon>Shamisen-NuGetIcon.png</PackageIcon>
    <Description>FLAC Decoder for Shamisen.</Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
    <Import Project="../../../Shamisen.Core.targets" />
    <PropertyGroup>
      <PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
    </PropertyGroup>
    <ItemGroup>
      <None Include="..\..\..\Shamisen-NuGetIcon.png">
        <Pack>True</Pack>
        <PackagePath>\</PackagePath>
      </None>
    </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\Shamisen.Core\Shamisen.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Service Include="{508349b6-6b84-4df5-91f0-309beebad82d}" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Parsing\FlacCrc16.table.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>FlacCrc16.table.tt</DependentUpon>
    </Compile>
    <Compile Update="Parsing\FlacCrc8.table.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>FlacCrc8.table.tt</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Parsing\FlacCrc16.table.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>FlacCrc16.table.cs</LastGenOutput>
    </None>
    <None Update="Parsing\FlacCrc8.table.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>FlacCrc8.table.cs</LastGenOutput>
    </None>

  </ItemGroup>
  <ItemGroup>
      <None Include="README.md" Pack="true" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Composing\" />
  </ItemGroup>
</Project>
